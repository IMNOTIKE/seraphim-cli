    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Launch Dump command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["db", "dmp"],
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Add Connection command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["db", "ac"],
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Edit Connection command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["db", "ec"],
            "console": "integratedTerminal"
        },
        {
            "name": "Launch Init Configuration command",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": ["cfg", "i"],
            "console": "integratedTerminal"
        }
    ]
}